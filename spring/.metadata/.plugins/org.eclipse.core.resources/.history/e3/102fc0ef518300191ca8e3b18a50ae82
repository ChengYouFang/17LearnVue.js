package com.demo;

import org.springframework.stereotype.Component;

@Component
public class Shop {
	private String name;
	private String pwd;
	//getter and setter methods
	public void set_name(String tmp)
	{
		this.name  = tmp;
	}
	public void set_pwd(String tmp)
	{
		this.pwd = tmp;
		
	}
	public void show_data()
	{
		System.out.print(name);
		System.out.print(pwd);
		
	}
}